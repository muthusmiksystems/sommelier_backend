{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "anandsiddharth/laravel-paytm-wallet": "^1.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "bavix/laravel-wallet": "^10.1",
        "berkayk/onesignal-laravel": "^1.10",
        "christiankuri/laravel-favorite": "^1.4",
        "doctrine/dbal": "^3.5",
        "fakerphp/faker": "^1.9.1",
        "firebase/php-jwt": "^6.10",
        "google/apiclient": "^2.15",
        "guzzlehttp/guzzle": "^7.8",
        "intervention/image": "^2.4",
        "ixudra/curl": "^6.16",
        "jackiedo/dotenv-editor": "^2.1",
        "jenssegers/agent": "^2.6",
        "kreait/firebase-php": "^7.9",
        "kreait/laravel-firebase": "^5.8",
        "lab404/laravel-impersonate": "*",
        "laravel/framework": "^10.0.15",
        "laravel/legacy-factories": "^1.3",
        "laravel/socialite": "^5.12",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/omnipay": "^3.2",
        "linfo/linfo": "^4.0",
        "maatwebsite/excel": "^3.1",
        "mews/captcha": "^3.3",
        "mike42/escpos-php": "2.2",
        "milon/barcode": "^10.0",
        "mjaschen/phpgeo": "3.0",
        "nesbot/carbon": " ~1.20|^2.62.0",
        "nwidart/laravel-modules": "^7.2",
        "omnipay/paypal": "dev-test-ghactions",
        "omnipay/stripe": "^3.0",
        "pcinaglia/laraupdater": "^1.0",
        "rap2hpoutre/laravel-log-viewer": "^2.3",
        "razorpay/razorpay": "2.*",
        "s-ichikawa/laravel-sendgrid-driver": "^4.0",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/geocoder": "^3.14",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-permission": "^5.8",
        "stripe/stripe-php": "^7.49",
        "twilio/sdk": "^5.36",
        "tymon/jwt-auth": "^2.1",
        "vinkla/hashids": "^11.0",
        "yajra/laravel-datatables-oracle": "^10.3",
        "zanysoft/laravel-zip": "^2.0"
    },
    "require-dev": {
        "appy/fcmhttpv1": "dev-master",
        "barryvdh/laravel-debugbar": "^3.8",
        "jzonta/faker-restaurant": "^2.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Http/notifyHelpers.php",
            "app/Http/helpers.php",
            "app/Helpers/Ver/Anton.php",
            "app/Helpers/Ver/Worker.php",
            "app/Helpers/Ver/NodeClient.php",
            "app/Helpers/Ver/Cluster.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    }
}
